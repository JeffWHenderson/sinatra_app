<h1>Show all characters</h1>
<table>
    <tr>
      <% Character.all.each do |c| %>
        <td><%= c.name %></td>
      <% end %>
    </tr>
    <tr>
      <% Character.all.each do |c| %>
        <td><%= c.user_id %></td>
      <% end %>
    </tr>
    <tr>
      <% Character.all.each do |c| %>
        <td><%= c.user_id %></td>
      <% end %>
    </tr>
    <tr>
      <% Character.all.each do |c| %>
        <td><%= User.find_by_id("#{c.user_id}").username %></td>
      <% end %>
    </tr>
</ul>
</table>
